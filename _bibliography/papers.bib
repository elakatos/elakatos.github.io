	doi = {10.1038/s41588-020-0687-1},
	id = {Lakatos2020},
	isbn = {1546-1718},
	journal = {Nature Genetics},
	number = {10},
	pages = {1057--1066},
	title = {Evolutionary dynamics of neoantigens in growing tumors},
	ty = {JOUR},
