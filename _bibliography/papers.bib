	doi = {https://doi.org/10.1093/annonc/mdz413.030},
	isbn = {0923-7534},
	journal = {Annals of Oncology},
	pages = {vii8},
	title = {25P - Low pass whole genome sequencing can be used to calculate the relative proportion of chemotherapy resistant disease in high grade serous ovarian cancer},
	ty = {JOUR},
